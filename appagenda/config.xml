<?xml version='1.0' encoding='utf-8'?>
<widget id="comjulianaoliveira.appsalvamento" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Agenda</name>
    <description>Agenda de contatos</description>
    <author email="juliana.odeus@hotmai.com" href="https://github.com/julianaoddeus">
        Criado com Apache Cordova 
    </author>
    <icon src="res/icon.png" />  
    <preference name="SplashScreen" value="screen" />
    <splash src="res/splash.jpg" />
    <preference name="SplashScreenDelay" value="3000" />  
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
</widget>
